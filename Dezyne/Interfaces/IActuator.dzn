interface IActuator {
	
	extern Pin $int$;	

	in void initialise(Pin pin);
	in void extend();
	in void withdraw();
	
	behaviour {
		enum State { Withdrawn, Extended };
		State state = State.Withdrawn;
		
		on extend: {state = State.Extended;}
		on withdraw: {state = State.Withdrawn;} 
	}
}

component Actuator
{
	provides IActuator actuator;
}